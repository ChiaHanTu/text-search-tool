{
  "name": "nuxt-app",
  "packageManager": "pnpm@10.6.5",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "typecheck": "nuxi typecheck",
    "lint:script": "eslint --ext .ts,.js,.vue --max-warnings=0",
    "lint:style": "stylelint '**/*.{vue,scss}' --ignore-path .gitignore node_modules",
    "lint:script:fix": "pnpm lint:script --fix",
    "lint:style:fix": "pnpm lint:style --fix",
    "lint": "pnpm lint:script && pnpm lint:style"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@pinia/nuxt": "^0.10.1",
    "@types/node": "^22.13.11",
    "@vueuse/core": "^13.0.0",
    "@vueuse/nuxt": "^13.0.0",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.86.0",
    "stylelint": "^16.16.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.8"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "@vorms/core": "^1.1.0",
    "@vorms/resolvers": "^1.1.0",
    "eslint": "^9.23.0",
    "nuxt": "3.16.1",
    "pinia": "^3.0.1",
    "tailwindcss": "^4.0.15",
    "vue-final-modal": "^4.5.5",
    "zod": "^3.24.2"
  },
  "lint-staged": {
    "./**/*.{js,ts,vue}": [
      "eslint --max-warnings=0"
    ],
    "./**/*.{scss,vue}": [
      "stylelint"
    ]
  }
}
